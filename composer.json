{
	"name": "mstorcha/matcha",
	"license": "proprietary",
	"type": "project",
	"description": "Matcha educational project",
	"minimum-stability": "dev",
	"keywords": ["server", "json", "matcha", "42"],
	"require": {
		"php": ">=7.4",
		"ext-json": "*",
		"ext-pdo": "*",
		"ext-memcached": "*",

		"php-di/slim-bridge": "dev-master",
		"slim/psr7": "^0.5",
		"vlucas/phpdotenv": "^3.6",
		"swiftmailer/swiftmailer": "^6.0",
		"monolog/monolog": "^1.24",

		"firebase/php-jwt": "^5.0",
		"madesimple/slim-auth": "dev-master",

		"cboden/ratchet": "^0.4.2",

		"ramsey/uuid": "4.x-dev",
		"symfony/serializer": "^5.0",
		"symfony/property-access": "^5.0",
		"gamez/ramsey-uuid-normalizer": "dev-master",
		"symfony/event-dispatcher": "^5.0",
		"yiisoft/data-response": "dev-master",
		"hiqdev/php-data-mapper": "dev-master"
	},
	"require-dev": {
		"vimeo/psalm": "^4.3",
		"nikic/php-parser": "^4.4.0",
		"phpunit/phpunit": "^9.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/",
			"App\\migrations\\": "migrations/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	}
