{
	"name": "mstorcha/matcha",
	"license": "proprietary",
	"type": "project",
	"description": "Matcha educational project",
	"keywords": ["server", "json", "matcha", "42"],
	"require": {
		"php": ">=7.1",
		"ext-json": "*",
		"ext-pdo": "*",
		"ext-memcached": "*",

		"slim/slim": "^4.1",
		"slim/psr7": "^0.5",
		"php-di/php-di": "^6.0",
		"monolog/monolog": "^1.24",
		"cboden/ratchet": "^0.4.2",
		"symfony/serializer": "^5.0",
		"symfony/property-access": "^5.0",
		"swiftmailer/swiftmailer": "^6.0",
		"ocramius/generated-hydrator": "^3.1",
		"vlucas/phpdotenv": "^3.6"
	},
	"require-dev": {
		"phpunit/phpunit": "^7.5"
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/",
			"App\\migrations\\": "migrations/",
			"tests\\": "tests/"
		}
	},
	"scripts": {
		"http": "php -S 0.0.0.0:8080 -t http http/index.php",
		"socket": "php socket/index.php",
		"test": "./vendor/bin/phpunit tests"
	}
